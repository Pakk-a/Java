
public class LinearEquationText {
	int a,b,c,d,e,f,x,y;
	LinearEquationText(){

	}
	LinearEquationText(int a,int b,int c,int d,int e,int f){
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
		this.e = e;
		this.f = f;
	}
	
	int getA(){
		return a;
	}
	
	int getB(){
		return b;
	}
	
	int getC(){
		return c;
	}
	
	int getD(){
		return d;
	}
	
	int getE(){
		return e;
	}
	
	int getF(){
		return f;
	}
	
	int getX(){
		return x = (e*d - b*f)/(a*d - b*c);
	}
	
	int getY(){
		return y = (e*c - a*f)/(a*d - b*c);
	}
	
	public void printLinear(){
		System.out.println("交点为：" + "(" +getX() + "," + getY() + ")");
	}
	
}
