import java.util.Scanner;

public class Seventhree {
	public static void main(String[] args) {
		System.out.println("Enter the integers betwwen 1 and 100:");
		int[] s = new int[110];
		int[] f = new int[110];
		Scanner input = new Scanner(System.in);
		int number = input.nextInt();
		while(number != 0){
			number = input.nextInt();
			s[number]++;
			f[number]++;
		}
		for(int i = 0;i <= 100; i++){
			if(f[i] != 0 ){
				if(s[i] == 1)
					System.out.println(i + " occurs " + s[i] + " time");
				else
					System.out.println(i + " occurs " + s[i] + " times");
			}
		}
	}

}
