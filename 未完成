import java.util.Scanner;
public class PerfectSquare {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Enter an integer m: ");
		int m = input.nextInt();
		int i = 2;
		int j = 0;
		int data[] = new int[100];
		while (true) {
			j++;
			if (m % i == 0) {
				data[j] = i;
				m /= i;
			} 
			else
				i++;
			if (m == 1)
				break;
		}
	}
}

